{
  "manifest_version": 2,
  "name": "The Counterbalance",
  "version": "1",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "icons": { "16": "./assets/load-leaning-16.png", "48": "./assets/load-leaning-48.png", "128": "./assets/load-leaning-128.png" },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ],
  "browser_action": { "default_popup": "index.html" },
  "permissions": ["tabs", "webNavigation", "<all_urls>"]
}
